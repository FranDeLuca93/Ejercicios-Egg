Algoritmo extra1VectoresAleatorios
	
	Definir v1, v2 Como real;
	Definir i Como entero;
	
	Dimension v1[5], v2[5];
	
	Para i <- 0 Hasta 4 Hacer
		v1[i] <- Azar(10);
		Escribir "[", v1[i], "]" Sin Saltar;
	FinPara
	
	Escribir "";
	
	Para i <- 0 Hasta 4 Hacer
		v2[i] <- Azar(10);
		Escribir "[", v2[i], "]" Sin Saltar;
	FinPara
	
	Escribir "";
	
FinAlgoritmo
